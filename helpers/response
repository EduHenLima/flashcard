-- Return all response
serverless invoke local --function get_response

-- Create response
serverless invoke local --function create_response --data '{
    "resposta": "resposta teste"
}'

-- Delete response
serverless invoke local --function delete_response --data '{"id_resposta": 1}'

--Update
serverless invoke local --function update_response --data '{
    "id_resposta": 2,
    "resposta": "Update resposta"
}'
