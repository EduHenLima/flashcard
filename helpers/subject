-- Return all subject
serverless invoke local --function get_subject

-- Create subject
serverless invoke local --function create_subject --data '{
    "id_categoria": 5,
    "tipo": "NORMAL",
    "nome_assunto": "Ingles",
    "descricao": "Aulas de Inglês",
    "ativo": 1
}'

-- Delete subject
serverless invoke local --function delete_subject --data '{"id_assunto": 1}'

--Update
serverless invoke local --function update_subject --data '{
    "id_assunto": 1,
    "id_categoria": 5,
    "tipo": "INTELIGENTE",
    "nome_assunto": "Ingles",
    "descricao": "Aulas de Inglês",
    "ativo": 1
}'
