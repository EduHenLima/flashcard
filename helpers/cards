-- Return all cards
serverless invoke local --function get_cards

-- id topic
serverless invoke local --function get_cards_by_topic --data '{
    "id_assunto": 2
}'

-- Create cards
serverless invoke local --function create_cards --data '{
    "pergunta": 2,
    "resposta": 2,
    "id_assunto": 2,
    "ativo": 1
}'

-- Delete cards
serverless invoke local --function delete_cards --data '{"id_cards": 1}'

--Update
serverless invoke local --function update_cards --data '{
    "id_cards": 2,
    "id_assunto": 2,
    "pergunta": 2,
    "resposta": 2,
    "ativo": 0
}'
